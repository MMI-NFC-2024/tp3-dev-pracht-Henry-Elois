---
import penguins from "../../assets/penguins.json";
import Layout from "../../layouts/Layout.astro";
import GraphicSpecies from "../../components/GraphicSpecies.astro";

const penguinsChinstrap = penguins.filter(
  ({ species }) => species === "Chinstrap"
);
---

<Layout>
  <h1 class="text-2xl mb-3">Les penguins Chinstrap</h1>
  <GraphicSpecies species="Chinstrap" />
</Layout>
